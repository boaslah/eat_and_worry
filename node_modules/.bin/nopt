#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/nopt/bin/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/nopt/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules:/home/kwagei/Documents/hacking/programming/blessing/node_modules:/home/kwagei/Documents/hacking/programming/node_modules:/home/kwagei/Documents/hacking/node_modules:/home/kwagei/Documents/node_modules:/home/kwagei/node_modules:/home/node_modules:/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/nopt/bin/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/nopt/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules:/home/kwagei/Documents/hacking/programming/blessing/node_modules:/home/kwagei/Documents/hacking/programming/node_modules:/home/kwagei/Documents/hacking/node_modules:/home/kwagei/Documents/node_modules:/home/kwagei/node_modules:/home/node_modules:/node_modules:/home/kwagei/Documents/hacking/programming/blessing/eat_and_worry/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
